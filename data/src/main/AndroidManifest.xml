<manifest
    xmlns:android="http://schemas.android.com/apk/res/android"
    package="net.yslibrary.monotweety.data"
    >

    <queries>
        <intent>
            <action android:name="android.intent.action.VIEW" />
            <data
                android:host="twitter.com"
                android:scheme="https"
                />
        </intent>
    </queries>
</manifest>
